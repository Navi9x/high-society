{
  "name": "tickets",
  "version": "1.0.0",
  "description": "Mobile-web event ticketing app with QR code generation and scanning",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "seed": "node db/seed-admin.js",
    "tunnel": "lt --port 3000",
    "event": "concurrently \"node server.js\" \"lt --port 3000 --print-requests\""
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.7",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "localtunnel": "^2.0.2"
  }
}
